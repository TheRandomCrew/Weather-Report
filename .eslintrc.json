{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "next"],
  "rules": {
    "linebreak-style" :0,
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "arrow-parens": "off",
    "import/prefer-default-export": "off",
    "@next/next/no-img-element": "off"
  }
}
